//buscar Custom Fields
SELECT  CreatedDate, CreatedBy.Name, DeveloperName, EntityDefinition.DeveloperName, TableEnumOrId, Id, LastModifiedBy.Name, LastModifiedDate, Length, NamespacePrefix, Precision, Scale,Description
FROM CustomField 
where NamespacePrefix = null

//buscar Custom Objects
select id, DeveloperName, CreatedBy.Name, CreatedDate, Description
from CustomObject 




// Apex Jobs
SELECT id,ApexClassId,Status,CreatedDate,ExtendedStatus FROM AsyncApexJob ORDER BY CreatedDate DESC NULLS FIRST


//E-Mails
SELECT Id,MessageDate,Subject,ToAddress FROM EmailMessage ORDER BY CreatedDate DESC NULLS FIRST


// Custom Apps
SELECT ApplicationId,NamespacePrefix, name, label, Type FROM AppMenuItem WHERE IsAccessible = true

