<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Value__c</fullName>
    <description>Summarizes in a simple string the permissions granted, this allows to simplify the general view of assigned permissions and their comparison, which is necessary because depending on the type of component the values of the permissions are different.</description>
    <externalId>false</externalId>
    <formula>CASE(RecordType.DeveloperName,
     &apos;tabSettings&apos;, CASE(TEXT(Visibility__c),&apos;pkl_Visible&apos;,&apos;V&apos;,&apos;pkl_Available&apos;,&apos;A&apos;,&apos;D&apos;),
     &apos;fieldPermissions&apos;, CASE(TEXT(FieldPermission__c),&apos;pkl_Read&apos;,&apos;R&apos;,&apos;pkl_Edit&apos;,&apos;RE&apos;,&apos;D&apos;),
     &apos;objectPermissions&apos;,CASE(TEXT(ObjectRead__c),
                        &apos;pkl_True&apos;,IF(ObjectCreate__c,&apos;CR&apos;,&apos;-R&apos;)+IF(ObjectEdit__c,&apos;E&apos;,&apos;-&apos;)+IF(ObjectDelete__c,&apos;D&apos;,&apos;-&apos;)+&apos; &apos;+CASE(TEXT(ObjectAllRecords__c),&apos;pkl_Sharing&apos;,&apos;SA&apos;,&apos;pkl_View&apos;,&apos;VA&apos;,&apos;pkl_Modify&apos;,&apos;MA&apos;,&apos;--&apos;),
                        &apos;pkl_Disable&apos;,&apos;D&apos;,
                        &apos;---- --&apos;),
     IF(Enable__c  , &apos;E&apos;, &apos;D&apos;)
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Common Permission　　　　　　　　　　　　　　
E=Enabled　　　　　　　　　　　　　　　　　　
D=Disable　　　　　　　　　　　　　　　　　　　　
Tab Permission　　　　　　　　　　　　　　　
V=Enabled　　　　　　　　　　　　　　　　　　
A=Available　　　　　　　　　　　　　　　　　
D=Disable　　　　　　　　　　　　　　　　　
Field Permission　　　　　　　　　　　　　　　
R=Read　　　　　　　　　　　　　　　　　　
RE=Read &amp; Edit　　　　　　　　　　　　　　　　　
D=Disable</inlineHelpText>
    <label>Value</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
